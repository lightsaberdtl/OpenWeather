{
    "defaultCommandTimeout": 10000,
    "reporter": "cypress-multi-reporters",
        "reporterOptions": {
            "reporterEnabled": "mochawesome",
            "mochawesomeReporterOptions": {
                "reportDir": "cypress/reports/mocha",
                "quite": true,
                "overwrite": false,
                "html": false,
                "json": true
            }
        },
    "viewportWidth": 1280,
    "viewportHeight": 720
}